<script type="text/javascript">
	//<![CDATA[
	function GoSearch() {
	    var searchText = document.getElementById('ctl00_GlobalHeader_MarketingHeaderControl1_search').value;
        searchText = CleanSearchText(searchText);
        if (searchText != 'keyword search') window.location.href = 'http://www.equityapartments.com/KeywordSearchResults.aspx?searchFor=' + searchText;
    }

    function CleanSearchText(rawInput) {
        var cleanText = '';
        var pattern = /[^a-z0-9',.-\s]/i;
        // check if pass regex 
        var result = pattern.exec(rawInput);
        
        if (result == null) {
            // good 
            cleanText = rawInput;
        }
        else {
            // failed regex. replace bad chars with spaces
            var charToTest;
            var charResult;
            for (var index = 0; index < rawInput.length; index++) {
                charToTest = rawInput.charAt(index);
                charResult = pattern.exec(charToTest);
                if (charResult == null) {
                    cleanText += charToTest;
                }
                else {
                    cleanText += ' ';
                }
            }
        }
        return cleanText;
    }
	//]]>
</script>

