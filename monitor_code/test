<script type="text/javascript">
    var xSellType= 'CrossSellLeadsYesCrossSellClassicYes';
    function IsValidGcCrossSellChecked () {
    
        var IsChecked = true;

        if($('#ctl00_Body_GuestCard1_CrossSellLeadsControl1_rptCSProperty_ctl00_uxCrossSellSelected')[0] == undefined) { 
            // no checkboxes exist
        	IsChecked = false;
    	}
    	else {
            if($('#ctl00_Body_GuestCard1_CrossSellLeadsControl1_rptCSProperty_ctl00_uxCrossSellSelected')[0].checked != true) {
                // first checkbox not checked. check for second checkbox.
    	        
                if($('#ctl00_Body_GuestCard1_CrossSellLeadsControl1_rptCSProperty_ctl01_uxCrossSellSelected')[0] == undefined) {
                    // no checkboxes exist
        	        IsChecked = false;
                    alert('Please select at least one property');    
                }
                else {
                    if($('#ctl00_Body_GuestCard1_CrossSellLeadsControl1_rptCSProperty_ctl01_uxCrossSellSelected')[0].checked != true) {  
                        // neither first nor second checkbox were checked.
                	        IsChecked = false;
                            alert('Please select at least one property');    
                    }                  
                }     	        
    	    }
    	}
        return IsChecked;
    }
</script>
