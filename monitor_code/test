<script type="text/javascript"  language="javascript">
    var tourTimes;
    var lastTourTimesMRI;
    var officeClosedDays;

    $(document).ready(function () {
        if (Boolean(true)) {
            tourTimes = '11:00 AM,5:00 PM~9:00 AM,6:00 PM~9:00 AM,6:00 PM~9:00 AM,7:00 PM~10:00 AM,6:00 PM~9:00 AM,6:00 PM~9:00 AM,5:00 PM';
            lastTourTimesMRI = '17:00,17:00,,,,,,,,,16:30,16:30';
            officeClosedDays = GetClosedDays(tourTimes); // global var for HideOfficeClosedDays
            var defaultTourDate = GetDefaultTourDate(tourTimes, lastTourTimesMRI);
            PopulateTourReserveTimes(defaultTourDate, tourTimes, lastTourTimesMRI);
            SetHiddenTourDate(defaultTourDate);
            SetDefaultHiddenTourTime();

            $('div#calendar').datepicker({ 
                altField: 'input#tourDate', 
                altFormat: 'yy-mm-dd', 
                onSelect: ChangeTourDate, 
                minDate: defaultTourDate, 
                defaultDate: defaultTourDate, 
                disabled:true,
                beforeShowDay: HideOfficeClosedDays
            });
        }
    });


    function toggleTour() {
        var hdnTourToggle = document.getElementById("ctl00_Body_GuestCard1_hdnTourToggle");
        var tourChecked = 0;
        var tourToggle = document.getElementById("uxTourToggle");

        if (tourToggle.checked == true) {
            tourChecked = 1;
            hdnTourToggle.value = "true";
        }
        else {
            tourChecked = 0;
            hdnTourToggle.value = "false";
        }

        var uxTourTime = document.getElementById("ctl00_Body_GuestCard1_uxTourTime");

        if (tourChecked == 1) {
            uxTourTime.disabled = false;             
            $( 'div#calendar' ).datepicker('enable');
            $('#tourDisclaimer').css('color','#222');
        }
        else {
            uxTourTime.disabled = true
            $( 'div#calendar' ).datepicker('disable');
            $('#tourDisclaimer').css('color','#ccc');
        }

    }


</script>
<!-- TFS 199 end -->
